{
  "name": "velotrack-online-graphics",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "tsc -w & vue-cli-service serve & NODE_ENV=dev nodemon dist/server/velotrack-online-graphics",
    "build": "vue-cli-service build && tsc"
  },
  "nodemonConfig": {
    "ignore": [
      "src/appConfig/appConfig.json"
    ],
    "watch": [
      "dist/**/*.js"
    ],
    "delay": 1000
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.13.0",
    "core-js": "^3.8.3",
    "cors": "^2.8.5",
    "papaparse": "^5.3.2",
    "ts-node": "^10.9.1",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "nodemon": "^2.0.20",
    "typescript": "~4.5.5"
  }
}
